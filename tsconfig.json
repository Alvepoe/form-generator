{
  "compilerOptions": {
    "outDir": "dist",
    "module": "es2020",
    "target": "es2017",
    "declaration": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx"
  }
}